--1.  
SELECT *
  FROM nomnom
    LIMIT 2;     
                    
__________________________________________________________________________________________________________________________________________________________________
--2. 
SELECT DISTINCT(neighborhood) 
  FROM nomnom;
       
___________________________________________________________________________________________________________________________________________________________________
--3. 
SELECT DISTINCT(cuisine)
  FROM nomnom;
    
__________________________________________________________________________________________________________________________________________________________________
--4.  
SELECT *
  FROM nomnom
    WHERE cuisine = 'Chinese';
    
__________________________________________________________________________________________________________________________________________________________________
--5. 
SELECT name, review
  FROM nomnom
    WHERE review >= 4;
    
__________________________________________________________________________________________________________________________________________________________________
--6.
SELECT name AS 'Fancy Restaurants'
  FROM nomnom
    WHERE cuisine = 'Italian' AND price = '$$$';
__________________________________________________________________________________________________________________________________________________________________
--7. 
SELECT name AS 'Trey remembers this'
  FROM nomnom
    WHERE name LIKE '%meatball%';
__________________________________________________________________________________________________________________________________________________________________
--8.
SELECT name AS 'Closeby restaurants'
  FROM nomnom
    WHERE neighborhood = 'Midtown' OR neighborhood = 'Downtown' OR neighborhood = 'Chinatown';
__________________________________________________________________________________________________________________________________________________________________
--9.
SELECT name AS 'Health Unreviewed'
  FROM nomnom 
    WHERE health IS NULL;
__________________________________________________________________________________________________________________________________________________________________
--10.
SELECT name AS 'Top 10 Best Reviewed Restaurants', review AS 'Rated'
  FROM nomnom
    ORDER BY review DESC
      LIMIT 10;
__________________________________________________________________________________________________________________________________________________________________
--11.
SELECT name, review,
    CASE
      WHEN review > 4.5 THEN 'Extraordinary'
      WHEN review > 4 THEN 'Excellent'
      WHEN review > 3 THEN 'Good'
      WHEN review > 2 THEN 'Fair'
      ELSE 'Poor'
    END AS New_Rating_System
FROM nomnom
ORDER BY 3;
